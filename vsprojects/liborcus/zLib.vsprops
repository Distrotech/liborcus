<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="zLib"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(ZLIB_INCLUDE_DIR)"
	/>
	<Tool
		Name="VCLinkerTool"
		AdditionalLibraryDirectories="$(ZLIB_LIB_DIR)"
	/>
	<UserMacro
		Name="ZLIB_INCLUDE_DIR"
		Value="C:\dev\installed\include"
	/>
	<UserMacro
		Name="ZLIB_LIB_DIR"
		Value="C:\dev\installed\lib"
	/>
</VisualStudioPropertySheet>
